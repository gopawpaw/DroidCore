<?xml version="1.0" encoding="UTF-8"?>
<appconfig>
	<!-- 是否是生产环境 -->
	<IS_PRODUCT valuse="false"/>
	
	<!-- 日志配置: debug(是否打开日志)；level(日志级别：v\d\i\w\e) -->
	<LOG debug="true" level="v"/>
	
	<!-- sdcard根路径 -->
	<SDCARD_ROOT valuse="sdcard_root"/>
	
	<!-- 下载目录 -->
	<DOWNLOAD valuse="download"/>
	
	<!-- SharedPreferences默认缓存文件名 -->
	<CACHE_FILE valuse="cache_file"/>
	
	<!-- 生产环境URL -->
	<HOST_PRD valuse="http://www.demo.com"/>
	<!-- 测试环境URL -->
	<HOST_STG valuse="http://127.0.0.1"/>
</appconfig>